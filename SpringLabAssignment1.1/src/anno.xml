<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
	 <!-- <context:component-scan base-package="com.cg.demo" />  -->
	<bean id="Employee" class="com.cg.demo.Employee">
		<property name="Id" value="1001"/>     <!-- setter Injection -->
		<property name="Name" value="Pranav"/>
		<property name="Salary" value="20000"/>
		<property name="BU" value="PES_BU"/>
		<property name="Age" value="21"/>
	</bean>
	 
	
	 
</beans>